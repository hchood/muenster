<%= form_for @company do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>

  <%= f.label "description" %>
  <%= f.text_field :description %>

  <%= f.label "industry" %>
  <%= f.text_field :industry %>

  <%= f.label "website" %>
  <%= f.text_field :website %>

  <%= f.label "location" %>
  <%= f.text_field :location %>

  <%= f.submit %>
<% end %>
